        @Html.ValidationSummary(true)


        <div style="background: rgba(0, 0, 0, 0) linear-gradient(to bottom, rgba(80, 80, 80, 1) 0%, rgba(0, 0, 0, 1) 100%) repeat fixed 0 0;">

            <div class="container ">
                <div class="box-wrapper" style="padding-top:50px;padding-bottom: 114px; min-height:300px !important;">
                    <div class="login_box">
                        <form>
                            <div class="form-group">
                                <label for="email">Username:</label>
                                @Html.TextBoxFor(model => model.UserID, new { @class = "form-control", required = "required", placeholder = "User Id" })

                            </div>
                            <div class="form-group">
                                <label for="email">Password:</label>
                                @Html.PasswordFor(model => model.Password, new { @class = "form-control", required = "required", placeholder = "Password" })
                            </div>
                            <div class="form-group">                                
                                <span class="error_msg">@ViewBag.Error</span>
                            </div>
                            <button type="submit" class="btn btn-primary pull-right">Submit</button>
                            
                        </form>
                    </div>
                    </div>
                </div>
            </div>
       

            
        
    }





