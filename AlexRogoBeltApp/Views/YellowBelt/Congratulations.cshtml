<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/Content/Images/favicon.ico" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>


    <script>

        $(document).ready(function () {
            localStorage.removeItem("EnvId");
            localStorage.removeItem("TemplateResponse");
            localStorage.removeItem("StepName");
            localStorage.removeItem("ProcessId");
            
            
        });
    </script>


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <title>TOCICO</title>
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/responsive.css" rel="stylesheet" />
</head>
<body class="body-wrapper">
    <div class="body-content">
        <div class="container">
            <div class="box-wrapper" style="background-image: linear-gradient(to top,#ace0f9 0%, #fff1eb 100%);">
                <div class="question-area">
                    <div class="text-center" style="padding-top:220px;">
                        <img src="~/Content/Images/Check_Circle.png" style="width:128px; " />
                        <h3>Thank you!</h3>
                        <br />
                        <h4>You have successfully completed Alex Rogo Yellow Belt.</h4>
                        <br />
                        <a href="@Url.Action("Dashboard", "Questionnaire", new { MemberId = Request.QueryString["MemberId"] })" class="btn btn-success">Go to Dashboard</a>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
            crossorigin="anonymous"></script>

</body>
</html>